{
  "version": 2,
  "cli": {
    "defaultCollection": "@nrwl/nest"
  },
  "defaultProject": "my-app",
  "projects": {
    "my-app": {
      "root": "apps/my-app",
      "sourceRoot": "apps/my-app/src",
      "projectType": "application",
      "targets": {
        "build": {
          "executor": "@nrwl/node:build",
          "outputs": [
            "{options.outputPath}"
          ],
          "options": {
            "outputPath": "dist/apps/my-app",
            "main": "apps/my-app/src/main.ts",
            "tsConfig": "apps/my-app/tsconfig.app.json",
            "assets": [
              "apps/my-app/src/assets"
            ]
          },
          "configurations": {
            "production": {
              "optimization": true,
              "extractLicenses": true,
              "inspect": false,
              "fileReplacements": [
                {
                  "replace": "apps/my-app/src/environments/environment.ts",
                  "with": "apps/my-app/src/environments/environment.prod.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "executor": "@nrwl/node:execute",
          "options": {
            "buildTarget": "my-app:build"
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/my-app/**/*.ts"
            ]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": [
            "coverage/apps/my-app"
          ],
          "options": {
            "jestConfig": "apps/my-app/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "my-second-app": {
      "root": "apps/my-second-app",
      "sourceRoot": "apps/my-second-app/src",
      "projectType": "application",
      "targets": {
        "build": {
          "executor": "@nrwl/node:build",
          "outputs": [
            "{options.outputPath}"
          ],
          "options": {
            "outputPath": "dist/apps/my-second-app",
            "main": "apps/my-second-app/src/main.ts",
            "tsConfig": "apps/my-second-app/tsconfig.app.json",
            "assets": [
              "apps/my-second-app/src/assets"
            ]
          },
          "configurations": {
            "production": {
              "optimization": true,
              "extractLicenses": true,
              "inspect": false,
              "fileReplacements": [
                {
                  "replace": "apps/my-second-app/src/environments/environment.ts",
                  "with": "apps/my-second-app/src/environments/environment.prod.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "executor": "@nrwl/node:execute",
          "options": {
            "buildTarget": "my-second-app:build"
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/my-second-app/**/*.ts"
            ]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": [
            "coverage/apps/my-second-app"
          ],
          "options": {
            "jestConfig": "apps/my-second-app/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "my-app-my-lib": {
      "root": "libs/my-app/my-lib",
      "sourceRoot": "libs/my-app/my-lib/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/my-app/my-lib/**/*.ts"
            ]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": [
            "coverage/libs/my-app/my-lib"
          ],
          "options": {
            "jestConfig": "libs/my-app/my-lib/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "my-app-my-second-lib": {
      "root": "libs/my-app/my-second-lib",
      "sourceRoot": "libs/my-app/my-second-lib/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/my-app/my-second-lib/**/*.ts"
            ]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": [
            "coverage/libs/my-app/my-second-lib"
          ],
          "options": {
            "jestConfig": "libs/my-app/my-second-lib/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    }
  }
}